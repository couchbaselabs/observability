      "gridPos": {
        "h": 5,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 2,
      "options": {
        "showHeader": true,
        "sortBy": []
      },
      "pluginVersion": "8.1.1",
      "targets": [
        {
          "cacheDurationSeconds": 300,
          "fields": [
            {
              "jsonPath": "$[*].nodes_summary[0].host",
              "name": "Cluster"
            },
            {
              "jsonPath": "$[*].name",
              "language": "jsonpath",
              "name": "Name"
            },
            {
              "jsonPath": "$map($, function($v) {$map($v.nodes_summary[*].version, function($m) {\n    $join($append($split($m, \"-\")[0], $replace($split($m, \"-\")[2], \"e\", \"E\", 1)), \" \")\n    \n})[0]})",
              "language": "jsonata",
              "name": "Version"
            },
            {
              "jsonPath": "$[*].nodes_summary.length",
              "language": "jsonpath",
              "name": "Nodes"
            },
            {
              "jsonPath": "$map($, function($v) {$join($sort($distinct($map($v.nodes_summary.services, function($m) {\n    $replace($replace($replace($m, \"kv\", \"Data\"), \"index\", \"Index\"), \"n1ql\", \"Query\")\n}))), ', ')})",
              "language": "jsonata",
              "name": "Services"
            },
            {
              "jsonPath": "$[*].status_summary.alerts",
              "language": "jsonpath",
              "name": "Critical"
            },
            {
              "jsonPath": "$[*].status_summary.warnings",
              "language": "jsonpath",
              "name": "Warnings"
            },
            {
              "jsonPath": "$[*].status_summary.good",
              "language": "jsonpath",
              "name": "Successful"
            },
            {
              "jsonPath": "$[*].status_summary.info",
              "language": "jsonpath",
              "name": "Info Alerts"
            }
          ],
          "method": "GET",
          "queryParams": "",
          "refId": "A",
          "urlPath": "/clusters"
        }
      ],
      "title": "Inventory of Couchbase Clusters",
