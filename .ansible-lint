---
skip_list:
  - line-length
  - comments       # Disable error on single space with comments
  - git-latest     # All this usage is in testing
  - package-latest
exclude_paths:
  - .cache/        # Implicit unless exclude_paths is defined in config
  - .github/
  - docs/
parseable: true
use_default_rules: true
verbosity: 1
